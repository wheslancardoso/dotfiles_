{
   "author": "compose.ai",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "assets/compose_logo_2x.png",
      "default_popup": "popup.html",
      "default_title": "Compose AI"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "general.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": false,
      "hostnames": [ "mail.google.com" ],
      "js": [ "gmail.js" ],
      "matches": [ "*://mail.google.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "hostnames": [ "docs.google.com" ],
      "js": [ "gdocsFallback.js" ],
      "matches": [ "*://docs.google.com/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "hostnames": [ "docs.google.com" ],
      "js": [ "gdocs.js" ],
      "matches": [ "*://docs.google.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": false,
      "hostnames": [ "mail.superhuman.com" ],
      "js": [ "superhuman.js" ],
      "matches": [ "*://mail.superhuman.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": false,
      "hostnames": [ "outlook.office.com", "outlook.office365.com", "outlook.live.com" ],
      "js": [ "outlook.js" ],
      "matches": [ "*://outlook.office.com/*", "*://outlook.office365.com/*", "*://outlook.live.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": false,
      "hostnames": [ "www.linkedin.com", "linkedin.com" ],
      "js": [ "linkedin.js" ],
      "matches": [ "*://www.linkedin.com/*", "*://linkedin.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": false,
      "hostnames": [ "guide.compose.ai" ],
      "js": [ "guide.js" ],
      "matches": [ "*://guide.compose.ai/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": false,
      "hostnames": [ "web.whatsapp.com" ],
      "js": [ "whatsapp.js" ],
      "matches": [ "*://web.whatsapp.com/*" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Accelerate your writing with AI",
   "externally_connectable": {
      "matches": [ "https://mail.google.com/*", "https://*.compose.ai/*", "https://compose-2.webflow.io/*" ]
   },
   "icons": {
      "128": "assets/compose_logo_128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Compose AI: AI-powered Writing Tool",
   "permissions": [ "tabs", "webNavigation", "https://*/", "management", "alarms", "clipboardWrite" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.3",
   "version_name": "1.3.3",
   "web_accessible_resources": [ "assets/compose_logo_128.png", "assets/logo-text.png", "assets/loader.svg", "assets/onboarding-assets/onboarding-tooltip-man.svg", "assets/michael.png", "assets/michael-face.png", "assets/wilson.png", "assets/promo_code_accent_bottom_right.png", "assets/promo_code_accent_top_left.png", "assets/rephrase_logo.png", "assets/logo-with-text.png", "assets/Inter-Light.ttf", "assets/Inter-Regular.ttf", "assets/Inter-Medium.ttf", "assets/Inter-SemiBold.ttf", "assets/Inter-Black.ttf", "assets/settings.svg" ]
}
